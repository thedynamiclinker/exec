#!/usr/bin/env bash

me="$0"

chars=()
only_kanji=
exact_match=

while [[ -n "$1" ]]; do
    case "$1" in
        --only-kanji)       only_kanji=1;;
        e|-e|--exact)       exact_match=1;;
        *)                  chars+=("$1");;
    esac
    shift
done

if [[ "$exact_match" == 1 ]]; then
    bef='\t'
    aft='(\t|$)'
else
    bef=''
    aft=''
fi

# Sets tabs to display with a width of 14.
# tabs 14

header="RTHn	RSHn	RTKn	TH	SH	JK	RTHw	RSHw	RTKw"
printf "${header}\n"
if [[ -n $only_kanji ]]; then
    for kanji in "${chars[@]}"; do
        grep -Pi "${bef}${kanji}${aft}" "$me" | cut -f2 | sed -r -z 's@\n@@g';
    done
    printf '\n'
else
    for kanji in "${chars[@]}"; do
        grep -Pi "${bef}${kanji}${aft}" "$me"
    done
fi


exit 0

